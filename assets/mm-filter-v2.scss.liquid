      max-width: 93%;
      margin: 0 auto 20px;
    }
    .search-box {
      margin: 0;
      position: relative;
      padding-top: 10px;
      
      input {
        width: 100%;
        border: 1px solid $border-color;
        padding-right: 40px;
        outline: none;
        
        &::placeholder {font-size: 14px;}
      }
      svg {
        position: absolute;
        top: 20px;
        right: 10px;
        opacity: 0.2;
        max-width: 20px;
      }
