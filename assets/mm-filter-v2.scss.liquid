      overflow-x: hidden;
      overflow-y: auto;
      
      @media (max-width: 767px) {
        max-height: unset;
      }
      .filter-item {
        padding: 0 0;
        margin: 5px 0px;
        cursor: pointer;
        line-height: 1.4em;
    	text-align: left;
        
        .filter-item-checkbox {
          height: 18px;
          width: 18px;
          display: inline-block;
          border: 3px solid #00BAC6;
          vertical-align: middle;
          margin-right: 5px;
          position: relative;
          
          &.color {
            height: unset;
            width: unset;
            
            span {
              border: 2px solid #ffffff;
              display: block;
              width: 20px;
              height: 20px;
            }
          }
        }
        .filter-item-name {
          font-size: 14px;
          vertical-align: middle;
          text-transform: capitalize;
          color: $primary-color;
        }
        &.filter-selected {
          .filter-item-checkbox:not(.color) {
            &:after {
              content: '';
              width: 14px;
    		  height: 14px;
              position: absolute;
              top: 0px;
              left: 0px;
              //border: .6mm solid #fff;
              //border-top: none;
              //border-right: none;
              background: #00BAC6;
              //transform: rotate(-50deg);
              box-sizing: border-box;
            }
