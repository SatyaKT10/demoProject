.mm-top-margin {
  padding-top: 20px;
  
  .flex-grid--collection-promotions {
    margin-bottom: 0;
  }
}
.mm-collection {
  padding-top: 0;
  
  @media(min-width: 769px) {
  	//padding-top: 45px;
  }
  .mm-collection-title {
  	margin-bottom: 20px;
    text-align: center;
    
    h2 {
      margin: 0;
      color: $primary-color;
    }
  }
  .mm-collection-description {
    width: 100%;
  	max-width: 700px;
    text-align: center;
    margin: 0 auto 0px;
    color: $text-color;
  }
  .mm-collection-breadcrumb {
  	text-align: left;
    margin: 0 auto 30px;
    font-size: 14px;
    color: #000;
      @media (max-width: 768px) {
      	margin: 30px auto 30px;
      }
    .collection-title{
    	color: #009DE0;
    }
  }
  .mm-product-header {
    display: -webkit-box; 
    display: -moz-box;
    display: flex;
    justify-content: space-between;
    margin-bottom: 20px;
    border-bottom: 1px solid #000;
    
    p {     
      margin: 0;
      font-size: 14px;
      font-style: italic;
      color: #000;
      padding: 10px 0;
      @media(max-width: 767px) {
        margin: 0 0 10px;
        font-size: 14px;
        text-align: left;
      }
    }
    
    .mm-sort-content {
      max-width: 100%;

      select {
        outline: none;
        width: 100%;
        font-size: 14px;
        border: none;
