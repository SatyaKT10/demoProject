/* RETAIL LOGIN PAGE CSS */

$montserrat: 'Montserrat', sans-serif;

::-webkit-input-placeholder { /* Edge */
  color: #d8d8d8;
}

:-ms-input-placeholder { /* Internet Explorer 10-11 */
  color: #d8d8d8;
}

::placeholder {
  color: #313131;
}

#retailer-login,#retailer-section {
  background: #f6f6f6;

  #shopify-section-retailer-login,.retailer-div {
    padding-top: 55px;

    .retailer__login,.retailer__update {

      .login__content {

        .tab__content {
          display: none;

          &.active {
            display: block;
          }

          h2 {
            margin-bottom: 1.5em;
            font-family: $montserrat;
            font-size: 28px;
            font-weight: 500;
            text-align: center;
            text-transform: capitalize;
            letter-spacing: 0;
            color: #000;
          }

          .tab__content--inner {
            padding: 30px;
            margin: 0 auto 30px auto;
            width: 100%;
            max-width: 540px;
            background: #FFF;
            border-radius: 5px;

            label {
              margin-bottom: 2em;
              width: 100%;
              display: block;
              cursor: default;

              > span {
                margin-bottom: 10px;
                display: block;
                font-family: $montserrat;
                font-weight: 500;
                font-size: .875em;
                color: #000;

                &.select2-container {
                  .select2-selection--multiple {
                    padding-right: 20px;
                    min-height: 40px;
                    border: 1px solid #eaeaea;
                    position: relative;
                    &:after {
                      border: 5px solid transparent;
                      border-top-color: #EAEAEA;
                      content: "";
                      display: inline-block;
                      position: absolute;
                      right: 10px;
                      top: 17px;
                    }
                  }
                }

                sup {
                  font-size: .875em;
                }
              }

              .select2-container--default .select2-selection--multiple .select2-selection__choice {
                padding: 5px 8px 5px 25px;
                background-color: #a4dade;
                border: 1px solid #2cb3be;
                font-weight: 300;
                color: #438388;
                position: relative;
              }

              .select2-container--default .select2-search--inline .select2-search__field {
                height: 30px;
              }

              .select2-container--default .select2-selection--multiple .select2-selection__choice__remove {
                position: absolute;
                left: 6px;
                top: 15px;
                font-size: 24px;
                line-height: 0;
                font-weight: 300;
                color: #438388;
              }

              .form-field {
                padding: 10px 15px;
                width: 100%;
                height: 42px;
                font-family: $montserrat;
                font-size: .875em;
                border: 1px solid #eaeaea;
                border-radius: 3px;
                outline: none;

                &.error {
                  border-color: #fdacac;
                }
              }

              input[type=number]::-webkit-inner-spin-button, 
              input[type=number]::-webkit-outer-spin-button { 
                -webkit-appearance: none; 
                margin: 0; 
              }

              button {
                margin-top: 10px;
                padding: 10px;
                width: 100%;
                height: 45px;
                background: #2cb3be;
                border-radius: 4px;
                border: none;
                outline: none;
                font-family: $montserrat;
                font-size: .875em;
                font-weight: 500;
                letter-spacing: 1px;
                color: #FFF;
                cursor: pointer;
                -webkit-transition: all .5s;
                -o-transition: all .5s;
                transition: all .5s;

                &:hover {
                  background: #22a4af;
                }
              }
            }

            .fieldset {
              display: flex;
              justify-content: space-between;

              label {
                flex: 0 0 48%;
              }
            }
          }
        }
      }
    }

  }

  .login__header {
    background: #FFF;

    .row {
      display: flex;

      @media (max-width: 767px) {
        display: block;
      }

      > div {
        padding: 10px 0;
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;

        &.logo {
          img {
            max-width: 250px;
          }
        }
      }
    }

    .login__tabs {
      ul {
        text-align: center;
        li {
          display: inline-block;
          width: 90px;
          position: relative;

          &:after {
            width: 66px;
            border-top: 2px solid #e2e2e2;
            display: inline-block;
            content: "";
            position: absolute;
            left: 60px;
            top: 7px;
          }

          &.tab_filled {
            &:after {
              border-color: #2cb3be;
            }

            a {
              color: #2cb3be;

              &:before {
                border-color: #2cb3be;
                background: #2cb3be;
              }
            }
          }

          &:last-child {
            &:after {
              display: none;
            }
          }

          a {
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            font-family: $montserrat;
            font-size: .875em;
            letter-spacing: 1px;
            cursor: default;

            &:before {
              margin-bottom: 5px;
              width: 16px;
              height: 16px;
              border-radius: 50%;
              border: 2px solid #e2e2e2;
              display: inline-block;
              content: "";
            }
          }

          &.active {
            a {
              font-weight: 600;
              color: #2cb3be;

              &:before {
                border-color: #2cb3be;
              }
            }
          }
        }
      }
    }
  }
}

