  }
}

.product-single {
  .price {
    align-items: flex-start;
  }
}

.price--unavailable {
  visibility: hidden;
}

.price__regular {
  display: block;
}

.price--on-sale {
  .price__regular, .price__availability {
    display: none;
  }
}

.price__availability {
  display: none;
}

.price--sold-out {
  .price__availability {
    display: block;
  }
  .price__regular, .price__sale, .price__unit {
