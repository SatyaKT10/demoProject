}

.product-card__title {
  font-size: 13px;
  color: #000;
  font-weight: 600;
  margin-top: 10px;
  line-height: 1.4;
  text-align: center;
}

.product-card__image-with-placeholder-wrapper {
  position: relative;
}

.grid-product__meta {
  text-align: center;
}

.grid-view-item__image-wrapper,
.grid-view-item__image {
