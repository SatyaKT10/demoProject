/* csutom checkbox */

.wallet-container,
.whatsapp__optin-checkbox,
.payOnDelivery-checkbox{
    display: block;
    position: relative;
    cursor: pointer;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 15px 15px 15px 15px;
    margin-bottom:0px;
}

/* Hide the browser's default checkbox */
.wallet-container input,
.whatsapp__optin-checkbox input,
.payOnDelivery-checkbox input{
  position: absolute;
  opacity: 0;
  cursor: pointer;
  height: 0;
  width: 0;
}

/* Create a custom checkbox */
.checkmark {
  position: absolute;
  top: 17px;
  left: 15px;
  height: 18px;
  width: 18px;
  background: #FFFFFF;
  border-radius: 2px;
  border: 2px solid #00BAC6;
}

/* On mouse-over, add a grey background color */
.wallet-container:hover input ~ .checkmark,
.whatsapp__optin-checkbox:hover input ~ .checkmark,
.payOnDelivery-checkbox:hover input ~ .checkmark{
  background-color: #FFFFFF;
}

/* When the checkbox is checked, add a blue background */
.wallet-container input:checked ~ .checkmark, 
.whatsapp__optin-checkbox input:checked ~ .checkmark,
.payOnDelivery-checkbox input:checked ~ .checkmark{
    background-color: #00BAC6;
}

/* Create the checkmark/indicator (hidden when not checked) */
.checkmark:after {
  content: "";
  position: absolute;
  display: none;
}

/* Show the checkmark when checked */
.wallet-container input:checked ~ .checkmark:after,
.whatsapp__optin-checkbox input:checked ~ .checkmark:after,
.payOnDelivery-checkbox input:checked ~ .checkmark:after{
  display: block;
}

/* Style the checkmark/indicator */
.wallet-container .checkmark:after, 
.whatsapp__optin-checkbox .checkmark:after,
.payOnDelivery-checkbox .checkmark:after{
  left: 4px;
  top: 0px;
  width: 6px;
  height: 12px;
  border: solid white;
  border-width: 0 2px 2px 0;
  -webkit-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  transform: rotate(45deg);
}


