<style>
  .#MainContent{
    padding-bottom:70px;
  }
  @media (min-width: 749px){
    .mobile-hide {
      display: block;
    }
  }
  @media (min-width: 749px){
    .desktop-hide {
      display: none!important;
    }
  }
  @media(max-width:749px){
    .desktop-hide {
      display: none!important;
    }
    .section-desc{
      display:none;
    }
    .section-heading,
    .section-sub-heading{
      text-align:center;
    } 
  }
  
  .model-canvas{
    display: none;
    border-width: 2px;
  }
  .model-camera__wrapper{
    position: relative;
  }
  .model-camera{
    position: absolute;
    top: 0;
    left: 0;
  }
  .model-annotation{
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
  }

  .hp__color-section{
    margin: 0px;
    border-radius: 4px;
    background-color: #FFFFFF;
  }
  .hp__color-section .section-container {
    margin: 0 auto;
    padding: 10px 0;
    position:relative;
  }
  .hp__color-section .section-container .absolute__image--1{
    position: absolute;
    top: 11%;
    left: 0;
    display: none;
  }
   .hp__color-section .section-container .absolute__image--2{
    position: absolute;
    top: 21%;
    right: 0;
    display: none;
  }
   .hp__color-section .section-container .absolute__mobile--1,
   .hp__color-section .section-container .absolute__mobile--2{
      display:none;
    }
  .hp__color-section .section-heading h2 {
    font-family: Quattrocento !important;
    font-style: normal;
    font-weight: bold;
    font-size: calc(24px + (45 - 24) * ((100vw - 375px) / (1440 - 375)));
    line-height: calc(27px + (50 - 27) * ((100vw - 375px) / (1440 - 375)));
    padding-top: calc(20px + (18 - 40) * ((100vw - 375px) / (1440 - 375)));
    padding-bottom: calc(2px + (5 - 2) * ((100vw - 375px) / (1440 - 375)));
    color: #474747;
  }
  .hp__color-section .section-sub-heading p {
    font-family: Poppins;
    font-style: normal;
    font-weight: 500;
    font-size: calc(12px + (14 - 12) * ((100vw - 375px) / (1440 - 375)));
    line-height: calc(18px + (21 - 18) * ((100vw - 375px) / (1440 - 375)));
    color: #666666;
    margin-bottom:10px;
  }
  .hp__color-section .section-desc p{
    font-family: Quattrocento !important;
    font-style: normal;
    font-weight: bold;
    font-size: calc(24px + (45 - 24) * ((100vw - 375px) / (1440 - 375)));
    line-height: calc(27px + (50 - 27) * ((100vw - 375px) / (1440 - 375)));
    padding-bottom: calc(2px + (5 - 2) * ((100vw - 375px) / (1440 - 375)));
    color: #474747;
    padding-top: calc(60px + (18 - 40) * ((100vw - 375px) / (1440 - 375)));
  }
  .hp__color-section .section-desc p span{
    font-family: Poppins;
    font-style: normal;
    font-weight: 500;
    font-size: calc(12px + (14 - 12) * ((100vw - 375px) / (1440 - 375)));
    line-height: calc(18px + (21 - 18) * ((100vw - 375px) / (1440 - 375)));
    color: #666666;
    display: block;
  }

  .hp__color-section .section-cta-btn-text{
    color: #FFFFFF;
    font-family: Poppins;
    font-style: normal;
    font-weight: 600;
    font-size: 16px;
    line-height: 24px;
    padding: 13px 0px;
    cursor: pointer;
  }
  .hp__color-section .section-cta-btn{
    display:none;
    width: 346px;
    height: 50px;
    margin: calc(15px + (30 - 15) * ((100vw - 375px) / (1440 - 375))) auto calc(15px + (20 - 15) * ((100vw - 375px) / (1440 - 375)));
    border-radius: 4px;
    background: #00BAC6;
    color: #FFFFFF;
    text-align: center;
  }
  
  .color__icon-wrapper {
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
    max-width: 100%;
    margin: 0 auto;
    padding: 0px;
    margin-top: calc(15px + (5 - 15) * ((100vw - 375px) / (1440 - 375)));
  }
  .hp__color-section .color_icon-content {
    outline:none;
    border:none;
    display: flex;
    align-items: center;
    width: 49%;
    background: #FFFFFF;
    box-shadow: 0px 4px 37px -4px rgb(143 139 139 / 12%);
    border-radius: 50px;
    padding: 5px;
    margin: 0;
    margin-top: calc(10px + (20 - 10) * ((100vw - 375px) / (1440 - 375)));
    cursor: pointer;
    z-index: 2;
  }
  .hp__color-section .color_icon-content.active {
    border: 1px solid #00bac6;
  }
  .hp__color-section .color_icon-content p {
    font-family: Poppins;
    font-style: normal;
    font-weight: 500;
    font-size: calc(12px + (14 - 12) * ((100vw - 375px) / (1440 - 375)));
    line-height: calc(18px + (21 - 18) * ((100vw - 375px) / (1440 - 375)));
    color: #666666;
    padding-left: 5px;
  }
  .hp__color-section .color_icon-content img {
    height: 40px;
    width: 40px;
  }
  .hp__color-section .model-content {
    display: none;
    margin: 0 auto;
  }
  .hp__color-section .model-content.active{
    display: block;
  }
  .hp__color-section .model-content img {
    width: 100%;
    height: 100%;
    object-fit: cover;
  }
  .try__on-content {
    position: relative;
    padding: 0 15px;
  }
  
  .ar-page__cta-wrapper{
    text-align: center;
  }
  
  .ar-page__retry-wrapper {
    display: flex;
    align-items: center;
    justify-content: space-between;
    gap: 15px;
    padding: 8px;
  }
  .capture__cta{
    width: 346px;
    height: 50px;
    margin: calc(15px + 15 * ((100vw - 375px) / 1065)) auto calc(15px + 5 * ((100vw - 375px) / 1065));
    border-radius: 4px;
    background: rgb(0, 186, 198);
    color: rgb(255, 255, 255);
    text-align: center;
    font-family: Poppins;
    font-style: normal;
    font-weight: 600;
    font-size: 16px;
    line-height: 24px;
    border:none;
  }
  .retry__cta{
    width: 346px;
    height: 50px;
    margin: calc(15px + 15 * ((100vw - 375px) / 1065)) auto calc(15px + 5 * ((100vw - 375px) / 1065));
    border-radius: 4px;
    color: #00BAC6;
    background: #FFFFFF;
    border: 1px solid #00BAC6;
    text-align: center;
    font-family: Poppins;
    font-style: normal;
    font-weight: 600;
    font-size: 16px;
    line-height: 24px;
  }
  
  .retry__cta[disabled]{
    cursor: default;
    color: #D8D8D8;
    border: 1px solid #D8D8D8;
  }
  
  .model__action--wrapper{
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-left: auto;
    padding: 15px;
    font-family: Poppins;
    font-style: normal;
    font-weight: 600;
    font-size: 12px;
    line-height: 18px;
    text-decoration-line: underline;
    color: #666666;
    display:none;
  }
  .model__share--wrapper{
    display: flex;
    align-items: center;
    gap: 8px;
    cursor: pointer;
  }
  .model__like--wrapper{
    display: flex;
    align-items: center;
    gap: 4px;
    cursor: pointer;
  }
  
  .model__like--wrapper .svg__active{
    color:red;
  }
    
  #breadcrumb{
    display: none;
  }
  .try__on-model{
    position: relative;
  }
  .model__loader--wrapper{
    display: none;
    position: absolute;
    bottom: 0;
    top: 0;
    left: 0;
    right: 0;
    width: 100%;
    height: 100%;
    z-index: 2;
  }
  
  .model__loader{
    width: 150px;
    height: 150px;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
  }
  
  .share-popup__container{
    display: none;
    width: 95%;
    max-width: 500px;
    box-shadow: 0 8px 16px rgb(0 0 0 / 15%);
    z-index: 999;
    border: 1px solid #ddd;
    padding: 20px;
    border-radius: 4px;
    background-color: #fff;
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
  }
  .share-popup__header{
    display: flex;
    justify-content: space-between;
    margin-bottom: 20px;
    border-bottom: 1px solid #ddd;
  }
  .share__header-title{
    font-family: Poppins;
    font-style: normal;
    font-weight: 500;
    font-size: 14px;
    line-height: 21px;
    color: #333333;
  }
  .share__popup-content{
    display: grid;
    grid-template-columns: 1fr 1fr;
    grid-gap: 20px;
    margin-bottom: 20px;
  }
  .share__button{
    display: inline-flex;
    align-items: center;
    justify-content: center;
    height: auto;
    padding: 5px;
    color: #666666;
    text-align: center;
    font-size: 14px;
    font-weight: 500;
    line-height: 1.1;
    letter-spacing: 2px;
    text-transform: capitalize;
    text-decoration: none;
    white-space: nowrap;
    border-radius: 4px;
    border: 1px solid #ddd;
    cursor: pointer;
  }
  .share__button span{
    margin-left: 7px;
  }
  .share-popup__close{
    cursor: pointer;
  }
  
  .model__info--wrapper {
    position: absolute;
    bottom: 0;
    top: 0;
    left: 0;
    right: 0;
    width: 100%;
    height: 100%;
    z-index: 2;
  }
  .model__info--container {
    width: 100%;
    height: 100%;
    background: rgba(0, 0,0, 0.3);
    margin-left: 60%;
    border-radius: 50%;
    overflow: hidden;
    padding: 25px;
    display: flex;
    align-items: center;
  }
  .model__info {
    display: flex;
    align-items: center;
    justify-content: space-between;
    gap: 10px;
    font-family: 'Poppins';
    font-style: normal;
    font-weight: 600;
    font-size: 14px;
    line-height: 21px;
    color: #FFFFFF;
    margin-bottom: 5px;
  }

  @media(min-width:749px) and (max-width:949px){
    .hp__color-section .section-container .absolute__image--1,
    .hp__color-section .section-container .absolute__image--2{
      display:none;
    }
    .hp__color-section .section-container .absolute__mobile--1,
    .hp__color-section .section-container .absolute__mobile--2{
      display:block;
    }
    .hp__color-section .section-container .absolute__mobile--1{
      position: absolute;
      left: 0%;
      top: 19%;
      display: none;
    }
    .hp__color-section .section-container .absolute__mobile--2{
      position: absolute;
      right: 0%;
      top: 28%;
      display: none;
    }
  }
  
  @media(min-width:1440px){
    .hp__color-section .section-heading h2{
      font-size: 45px;
    }
    .hp__color-section .section-desc p{
      font-size: 45px;
    }
  }
  
  @media(min-width:749px){
    .hp__color-section .section-container {
      max-width: 1200px;
      margin: 0 auto;
      display: flex;
      margin-top: 30px;
    }
    .hp__color-section .section-heading h2{
      text-align: center;
    }
    .hp__color-section .section-sub-heading p{
      text-align: center;
    }
    .hp__color-section .section-desc p{
      padding-top: 0px;
    }
    .try__on-model {
      width: 50%;
    }
    
    .try__on-content {
      width: 50%;
    }
    
    .model-wrapper .color__icon-wrapper{
      display: none;
    }
    
    .try__on-content .color__icon-wrapper{
      height: 310px;
      overflow: auto;
    }
    .model__info--wrapper {
      display: none;
    }
  }
  
  @media(max-width:749px) { 
    .hp__color-section{
      margin: 0px;
      border-radius: 4px;
    }
    .hp__color-section .section-container {
      padding: 0;
      padding-bottom: 15px;
    }
    .hp__color-section .section-container .absolute__image--1,
    .hp__color-section .section-container .absolute__image--2{
      display:none;
    }
    .hp__color-section .section-container .absolute__mobile--1,
    .hp__color-section .section-container .absolute__mobile--2{
      display:block;
    }
    .hp__color-section .section-container .absolute__mobile--1{
      position: absolute;
      left: 0%;
      top: 0%;
    }
     .hp__color-section .section-container .absolute__mobile--2{
      position: absolute;
      right: 0%;
      top: 28%;
    }
    .hp__color-section .ar-page__cta-wrapper{
      margin-bottom: 0px;
      margin-top: 0px;
      position: fixed;
      bottom: 0;
      left: 0;
      z-index: 99;
      width: 100%;
      padding: 0px;
      background: #FFFFFF;
      box-shadow: 0px 0px 4px rgb(0 0 0 / 25%);
    }
    .capture__cta {
      border:none;
      margin-top: 0;
      width: 100%;
      margin-bottom: 0;
    }
    .retry__cta {
      margin-top: 0;
      margin-bottom: 0;
    }
    .hp__color-section .section-cta-btn {
      margin-bottom: 0;
      margin-top: 0;
    }
    .model-canvas{
      padding: 0 15px;
    }
    
    .model-wrapper {
      position: relative;
    }
    .model-wrapper .color__icon-wrapper{
      position: absolute;
      bottom: 0;
      flex-wrap: nowrap;
      overflow:hidden;
      overflow-x: scroll;
      white-space: nowrap;
      -ms-overflow-style: none; 
      scrollbar-width: none;
      gap: 5px;
      padding-bottom: 10px;
    }
    
    .model-wrapper .color_icon-content{
      min-width: calc(168px + (178 - 168) * ((100vw - 375px) / (1440 - 375)));
    }
    
    .try__on-content .color__icon-wrapper{
      display: none;
    }
    
    .model-wrapper .color_icon-content.active{
      border: 2px solid #00bac6;
    }
    
    .model-wrapper .color__icon-wrapper::-webkit-scrollbar {
      display: none;
    }
  }

</style>

