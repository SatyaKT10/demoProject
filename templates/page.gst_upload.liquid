{% for tag in customer.tags %}
{% if tag contains 'gstNumber' %}
{% assign gstNo = tag %}
{% else %}
{% endif %}
{% endfor %}

{% assign bypassGst = false %}
{% for tag in customer.tags %}
{% if tag contains 'gstSkip_true' %}
{% assign bypassGst = true %}
{% endif %}
{% endfor %}


<script>
  $(document).ready(function () {   
    let searchParams = new URLSearchParams(window.location.search);
    let param = searchParams.get('update');
    let paramprofile = searchParams.get('updateprofile');

    if(param ){
      console.log("hel");
      $(".document_upload").removeClass("verifying");
      //       $(".sk-step").css("display", "none");


    }
    else if(paramprofile){
      $(".sk-step").css("display", "none");
      $(".bk-step").css("display", "block");
    }
    else if(!param){
      console.log("hhii");
      if($('#form--section').attr('gstNo') ){
        document.location.href = '/';
      }
      else{
        $(".document_upload").removeClass("verifying");
      }
    }

  })
</script>


