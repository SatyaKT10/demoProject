<script>
 $(document).ready(function () {
   if($('#form--section').attr('data-id') ==''){
         document.location.href = '/account';
     }
   })
 $(document).on('click','.sk-step',function(e){
   e.preventDefault();
   var thisE = $(this);
   var url = thisE.attr('href');
   sessionStorage.setItem("skipB",true);
   location.href = url;
   
 })
</script>