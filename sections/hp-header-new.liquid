  {% if section.settings.message %}
    {% if section.settings.home_page_only == false or request.page_type == 'index' %}
      <style>
        .announcement-bar {
          background-color: {{ section.settings.color_bg }};
        }
        .announcement-bar__message {
          color: {{ section.settings.color_text }};
          text-transform: inherit;
        }
      </style>  

        <div class="announcement-bar">
          <div class="page-width page-announcement-bar">
            <div class="announcement-bar--left">
                <div class="announcement-bar__message">{{ section.settings.message_text }}</div>
            </div>
            <div class="announcement-bar--right">
              {% assign message = section.settings.message_texts %}
              {% if message != blank %}
              <div class="announcement-bar--right--link mmlink1"><a href="{{ section.settings.message_link }}" class="announcement-bar--link">{{ message }}</a></div>
              {% endif %}
              {% if customer %}
              {% for tag in customer.tags %}
              {% if tag contains 'shopName' %}
              {% assign shopName = tag | split: "_" |last %}
              {% endif %}
              {% endfor %}
              <div class="site-nav__item-hover">
                <div class="announcement-bar--right--link mmlink2"><a href="/account"><span><img src="{{ 'user.png' | asset_img_url }}" /></span>Hi, <span class="customer-name"> {% if shopName %}{{shopName}}{% else %} &nbsp;Aqua partner{% endif %}</span></a></div>
                <div class="account-menu">
                  {% render 'account-navigation' %}
                </div>
              </div>
              {% else %}
              <div class="announcement-bar--right--link mmlink2"><a href="/account/login"><span><img src="{{ 'user.png' | asset_img_url }}" /></span>SIGN IN</a></div>
              {% endif %}
              <div class="announcement-bar--right--link mmlink3"><a href="/cart"><span><img src="{{ 'cart-icon.png' | asset_img_url }}" /></span>CART<span class="cart-count">({{ cart.item_count }})</span></a></div>
            </div>
          </div>
        </div>       
      

    {% endif %}
  {% endif %}

