{% schema %}
  {
    "name": "Aqualens Partner",
    "settings": [
    	{
          "type": "text",
          "id": "section-heading",
          "label": "Section Heading",
          "default": "Why partner with Aqualens? "
        }
	],
    "blocks": [
        {
          "name": "Partner Step",
          "type": "partner_slider",
		  "limit":3,
          "settings": [
			{
              "type": "image_picker",
              "id": "step_image",
              "label": "Image"
            },
            {
              "type": "text",
              "id": "partner_text",
              "label": "Text",
              "default": "1"
            }
          ]
        }
      ],

    "presets":[ 
            { 
               "name":{ 
                  "en":"Aqualens Partner"
               },
               "category":{ 
                  "en":"Homepage"
               }
            }
       ]
  }
{% endschema %}
