<style>
  .setion__separator {
    height: 48px !important;
    background: #F8FAFF;
    width: 100%;
    border: none;
    margin: 0px 0;
    opacity:1;
  }

  .aq-partner__section{
    background: white;
    
  }
  .aq-partner__section .partner__section--header {
    padding: 72px 0 60px 0;
  }
  .aq-partner__section .partner__section--header h2{
    font-family: 'Poppins';
    font-style: normal;
    font-weight: 600;
    font-size: 28px;
    line-height: 149.3%;
    text-align: center;
    color: #303030;
    letter-spacing:0;
    text-transform: lowercase;
  }
  .aq-partner__section .partner_cards_wrapper {
    display: flex;
    max-width: 1341px;
    margin: auto;
    gap: 30px;
    padding: 0 15px;
    border-bottom: 1px solid rgba(102, 102, 102, 0.11);
  }
  .aq-partner__section .partner_cards {
    flex: 0 0 calc(33.33% - 20px);
    width: calc(33.33% - 20px);
    position: relative;
    padding-bottom: 85px;
  }
  .aq-partner__section .partner_image {
    position: relative;
    padding-bottom: 90%;
    width: 100%!important;
    margin-bottom: 78px;
  }
  .aq-partner__section .partner_image img{
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 100%;
    height: 100%;
    border-radius: 4px 4px 0px 0px;
    object-fit: contain;

  }
  .aq-partner__section .partner_text {
    font-family: 'Poppins';
    font-style: normal;
    font-weight: 600;
    font-size: 16px;
    line-height: 149.3%;
    text-align: center;
    color: #666666;
    text-transform: capitalize;
    margin-bottom: 24px;
    text-align:center;
  }
  .aq-partner__section .icon_wrapper {
    display: flex;
    justify-content: space-between;
    align-items: center;
    max-width: 800px;
    margin: auto;
    margin-top: 87px;
    margin-bottom: 80px;
  }
  .aq-partner__section .aq_partner_icon {
    text-align: center;
  }
  .aq-partner__section .aq_partner_icon_text {
   margin-top: 26px;
    font-family: 'Poppins';
    font-style: normal;
    font-weight: 500;
    font-size: 12px;
    line-height: 149.3%;
    text-align: center;
    color: #2C2B2B;
  }
  @media (min-width: 749px) and (max-width: 949px){
    .aq-partner__section .partner_cards_wrapper {
      flex-wrap:wrap;
      justify-content:center;
    }
    .aq-partner__section .partner_cards {
      flex: 0 0 calc(50% - 20px);
      width: calc(50% - 20px);
      position: relative;
      padding-bottom: 85px;
      padding:0 32px 30px 32px; 
    }
     .aq-partner__section .icon_wrapper {
       max-width: unset;
       padding:0 15px;
       margin-top: 72px;
       margin-bottom: 60px;
    }
  }
  @media(max-width:749px){
    .aq-partner__section .partner__section--header {
      padding: 40px 32px 50px 32px;
    }
    .aq-partner__section .partner_cards_wrapper {
      flex-direction: column;
      flex-wrap:wrap;
    }
    .aq-partner__section .partner_cards {
      flex: 0 0 100%;
      width: 100%;
      position: relative;
      padding-bottom: 85px;
      padding:0 32px 30px 32px; 
    }
    .aq-partner__section .icon_wrapper {
      max-width: unset;
      padding:0 15px;
      margin-top: 72px;
      margin-bottom: 60px;
    }
    .aq-partner__section .aq_partner_icon_text {
      font-size: 10px;

    }
  }
  
</style>
{% assign partner_slider = section.blocks | where: 'type', 'partner_slider' %}
<hr class="setion__separator">
<section data-section-id="{{ section.id }}" data-section-type="aq_partner-section" class="aq-partner__section" id="partnerSection">
  
  
  {% if section.settings.section-heading != blank %}
  <div class="partner__section--header">
    <h2>{{ section.settings.section-heading }}</h2>
  </div>
  {% endif %}
  
  <div class="partner_cards_wrapper">
    
    {% for block in partner_slider  %}
    <div class="partner_cards">
      {% if block.settings.step_image != ''%}
      <div class="partner_image">
        <img class="lazyload" src="{{block.settings.step_image | img_url :'master'}}" data-src="{{block.settings.step_image | img_url :'master'}}" alt="Image"  />
      </div>
      {% endif %}
      {% if block.settings.partner_text != blank %}
      <div class="partner_text">{{ block.settings.partner_text }}</div>
      {% endif %}
    </div>
    {% endfor %}
    
  </div>

