      {% elsif template.suffix contains 'tax-statement' or template.suffix contains 'tax-request' %}
      <span class="cart__heading">Tax Statement</span>
      {% elsif template.suffix contains 'transactions' %}
      <span class="cart__heading">My Transactions</span>
      {% elsif template.suffix == 'customer-orders' or template.suffix == 'customer-order-details'%}
