<style>
  .added_nav{
    top:30px;
    border-radius: 10px;
    box-shadow: 0 4px 20px 1px rgb(0 0 0 / 15%), 0 1px 4px rgb(0 0 0 / 8%);
    border: 0;
  }
  .nav__mobile--show{
    display:none;
  }

  @media(max-width:749px){
    .nav__mobile--show{
      display:block;
    }
    .nav_desktop--show{
      display:none !important;
    }
  }
  .account-menu{
    display:none;
    width: 270px;
    height: auto;
    position: absolute;
    right: 8px;
    top: 12px;
    z-index: 999;
   
  }
  
  .my__name--number {
    text-align: left;
    text-transform: capitalize;
  }
  .site-nav__item-hover{
    border-radius:20px;
    position:relative;
  }
  h2.sign__warning {
    display: none;
    width: 100%;
    text-transform: capitalize;
    color: #ef7474;
    padding: 10px;
  }
  .sign__style {
    background: #00BAC6;
    color: white;
  }
  @media(min-width:749px){
    .grid__width{
      width:30% !important;
    }
  }
</style>
{% if section.settings.message %}
  <style>
    {% if section.settings.logo != blank %}
      .site-header__logo-image {
        max-width: {{ section.settings.logo_max_width | escape }}px;
      }
    {% endif %}

    {% if section.settings.align_logo == 'left' %}
      .site-header__logo-image {
        margin: 0;
      }
    {% endif %}
  </style>
{% endif %}


{% if template contains 'cart' or template.suffix contains 'freePrd' or template.suffix contains 'ar-filter' or template.suffix contains 'retailer-dashboard' or template.suffix contains 'order-reports' or template.suffix contains 'order-details' or template.suffix contains 'orders' or template.name contains 'order' or template.suffix contains 'tax-statement' or template.suffix contains 'tax-request' or template.suffix contains 'transactions' or template.suffix contains 'customer' %}

<div class="nav__mobile--show">
  <div class="cart__header">
