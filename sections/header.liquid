                  <span class="mobile-nav__label">Sign In</span>
                </a>                
                {% endif %}
                
              </ul>
            {% else %}
            
            {% assign linkName = link.title | downcase %}  
            {% assign varified = false %}
            {% if  linkName == 'catalog'%}
              {% if customer %}
                <a href="/collections/all"
                class="mobile-nav__link{% if link.active %} mobile-nav__link--active{% endif %}"
                {% if link.current %} aria-current="page"{% endif %}
              >
                <span class="mobile-nav__label">{{ link.title | escape }}</span>
              </a>
            	{% else %}
            	<a href="/account/login"
                class="mobile-nav__link{% if link.active %} mobile-nav__link--active{% endif %}"
                {% if link.current %} aria-current="page"{% endif %}
              >
                <span class="mobile-nav__label">{{ link.title | escape }}</span>
              </a>
              {% endif %}
            {% elsif  linkName == 'customer orders'%}
              {% if customer %}
                <a href="{{ link.url }}"
                class="mobile-nav__link{% if link.active %} mobile-nav__link--active{% endif %}"
                {% if link.current %} aria-current="page"{% endif %}
              >
                <span class="mobile-nav__label">{{ link.title | escape }}</span>
              </a>
            	{% else %}
            	<a href="/account/login"
                class="mobile-nav__link{% if link.active %} mobile-nav__link--active{% endif %}"
                {% if link.current %} aria-current="page"{% endif %}
              >
                <span class="mobile-nav__label">{{ link.title | escape }}</span>
              </a>
              {% endif %}
            {% else %}
            <a href="{{ link.url }}"
                class="mobile-nav__link{% if link.active %} mobile-nav__link--active{% endif %}"
                {% if link.current %} aria-current="page"{% endif %}
              >
                <span class="mobile-nav__label">{{ link.title | escape }}</span>
              </a>
            {% endif %}
            
            
              
            {% endif %}
          </li>
           
        {% endfor %}
        {% if customer %}
            <li class="mobile-nav__item{% unless forloop.last %} border-bottom{% endunless %}">
             
              <a href="/account" class="mobile-nav__link {% if link.active %} mobile-nav__link--active{% endif %}" aria-current="page">
                <span class="mobile-nav__label">Hi, {% if shopName %}{{shopName}}{% else %} &nbsp;Aqua partner {% endif %}</span>
              </a>
            </li>
        {% else %}
          <li class="mobile-nav__item{% unless forloop.last %} border-bottom{% endunless %}">
            <a href="/account/login" class="mobile-nav__link {% if link.active %} mobile-nav__link--active{% endif %} " aria-current="page">
                <span class="mobile-nav__label">Sign In</span>
