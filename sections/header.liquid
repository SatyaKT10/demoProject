      {% elsif template.suffix contains 'freePrd'%}
      <span class="cart__heading">Free Products</span>
      {% elsif template.suffix contains 'ar-filter'%}
      <span class="cart__heading">AR Filter</span>
      {% elsif template.suffix contains 'retailer-dashboard'%}
      <span class="cart__heading">Retailer Dashboard</span>
      {% elsif template.suffix contains 'order-details'%}
      <span class="cart__heading">My Orders</span>
      {% elsif template.suffix contains 'order-reports'%}
