        <div class="aqua-header-link mmHeaderlink2"><a href="/cart"><img src="{{ 'Vector_3.png' | asset_img_url }}" />&nbsp;&nbsp;Cart<span class="cart-count">({{ cart.item_count }})</span></a></div>
