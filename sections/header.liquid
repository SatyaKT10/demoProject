        <div class="aqua-header-link mmHeaderlink"><a href="/account" class="site-nav__item-hover"><img src="{{ 'Vector_2.png' | asset_img_url }}" /><span class="customer-name"> {% if shopName %}{{shopName}}{% else %} &nbsp;Aqua partner{% endif %}</span></a></div>

